
import java.io.*;

public class Ex2 {
public static void main(String[] args) throws IOException {
	FileReader f=new FileReader("new.txt");
	BufferedReader b=new BufferedReader(f);
	int i=1;
	String line=b.readLine();
	while(line!=null)
	{
		System.out.println(i+" "+line);
		line=b.readLine();
		i++;
	}
	b.close();
	f.close();
	
}
}
